def filter_by_type(t1, t2):
 """Finding the elements present in both of 2 tuples
 Args:
 t1: input tuple 1
 t2: input tuple 2
 Returns:
 (tuple): the intersection of the input tuples
 """
 result = ()
 for element in t1:
     if element in t2:
         result += (element,)
     return result

t1 = (1,"dog",False)
t2 = (2, "dog", 1.0)
print(filter_by_type(t1, t2))
