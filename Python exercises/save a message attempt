def write_to_file(writing):
    writing = open("something.txt", "w+")
    writing.write("elephant")
    return writing
     


def cypher(writing):
    word = write_to_file(writing)
    word_length = len(word)

    ciphered = ""
    for i in range(word_length):
        old = ord(word[i])
        new=old+1
        ciphered +=chr(new)
        i+=1
    
    return ciphered

